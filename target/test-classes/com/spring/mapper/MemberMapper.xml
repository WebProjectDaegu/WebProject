<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.mapper.MemberMapper">
	<insert id="registermember">
		insert into memberinfo (membern,id,password,name,gender,nickname,birth,phone) 
		values(memberseq.nextval, #{id},#{password},#{name},#{gender},#{nickname},#{birth},#{phone})
	</insert>
	<select id="login">
		select nickname from memberinfo where id = #{id} and password =#{password}
	
	</select>
	<select id="logincheck" resultType="int">
		select count(id) from memberinfo where id = #{id} and password =#{password}
	</select>
	<select id="registermembercheck" resultType="int">
		select count(id) from memberinfo where id = #{id}
	</select>

</mapper>